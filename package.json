{
  "name": "crewhrm",
  "version": "1.0.0",
  "description": "HR Management System",
  "main": "index.js",
  "scripts": {
    "pro-addons": "node ./components/builders/get-pro-addons.js",
    "compile": "npm run pro-addons && webpack --env=build --mode=production",
    "build": "npm run pro-addons && npm run compile && gulp build",
    "watch": "npm run pro-addons && webpack --watch --mode=development",
    "lint": "eslint ./components/**/*.jsx",
    "lint:fix": "eslint --fix ./components/**/*.jsx && prettier --write './components/**/*.{jsx,scss}' --config ./.prettierrc",
    "cypress:open": "cypress open --config-file tests/cypress/config.config.js",
    "cypress:run": "cypress run --config-file tests/cypress/config.config.js",
    "env:start": "wp-env start",
    "env:stop": "wp-env stop",
    "mocha": "npx mochawesome-merge ./tests/cypress/reports/*.json -o tests/cypress/reports/mochawesome.json && rm -rf ./tests/cypress/reports/mochawesome-*.json && npx mochawesome-json-to-md -p ./tests/cypress/reports/mochawesome.json -o ./tests/cypress/reports/mochawesome.md && npx mochawesome-report-generator tests/cypress/reports/mochawesome.json -o tests/cypress/reports/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CrewHRM/CrewHRM.git"
  },
  "keywords": [
    "HR",
    "HRM",
    "Recruitment",
    "Job Listings",
    "Employee management",
    "Leave management",
    "Attendance",
    "Reimbursement",
    "Human Resource Management"
  ],
  "author": "CrewHRM",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/CrewHRM/CrewHRM/issues"
  },
  "homepage": "https://github.com/CrewHRM/CrewHRM#readme",
  "dependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "@wordpress/env": "^8.6.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.3",
    "crewhrm-materials": "github:CrewHRM/Materials",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cypress": "^11.2.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-mochawesome-reporter": "^3.5.1",
    "draft-js": "^0.11.7",
    "draftjs-to-html": "^0.9.1",
    "fs": "^0.0.1-security",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-plumber": "^1.2.1",
    "gulp-wp-pot": "^2.5.0",
    "gulp-zip": "^5.1.0",
    "html-to-draftjs": "^1.5.0",
    "node-sass": "^9.0.0",
    "path": "^0.12.7",
    "pdfjs-dist": "^3.9.179",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-helmet": "^6.1.0",
    "react-image-zooom": "^1.2.0",
    "react-paginate": "^8.2.0",
    "react-router-dom": "^6.14.2",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "engines": {
    "node": ">=18.8.0"
  },
  "devDependencies": {
    "@10up/cypress-wp-utils": "github:10up/cypress-wp-utils#build",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gulp-notify": "^4.0.0",
    "prettier": "^3.0.2"
  }
}
